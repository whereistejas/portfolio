{
	"name": "whereistejas",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "bunx --bun astro dev",
		"start": "bunx --bun astro dev",
		"check": "bunx --bun astro check",
		"build": "bunx --bun astro build",
		"build:queue": "bun src/content/llm.ts",
		"preview": "bunx --bun astro preview",
		"astro": "bunx --bun astro",
		"prettier": "prettier --write .",
		"format:check": "prettier --check ."
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.71.2",
		"@astrojs/check": "^0.9.6",
		"@astrojs/ts-plugin": "^1.10.6",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.2.0",
		"astro": "5.17.3",
		"dotenv": "^16.6.1",
		"exifreader": "^4.36.2",
		"rehype-stringify": "^10.0.1",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"tailwindcss": "^4.2.0",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"unified": "^11.0.5"
	},
	"devDependencies": {
		"@types/bun": "^1.3.9",
		"zod": "^4.3.6",
		"prettier": "3.6.2",
		"prettier-plugin-astro": "^0.13.0",
		"prettier-plugin-tailwindcss": "^0.6.14"
	},
	"prettier": {
		"printWidth": 80,
		"tabWidth": 2,
		"useTabs": true,
		"semi": true,
		"singleQuote": false,
		"quoteProps": "as-needed",
		"trailingComma": "es5",
		"bracketSpacing": true,
		"arrowParens": "always",
		"endOfLine": "lf",
		"plugins": [
			"prettier-plugin-astro",
			"prettier-plugin-tailwindcss"
		],
		"overrides": [
			{
				"files": "*.astro",
				"options": {
					"parser": "astro"
				}
			}
		]
	},
	"private": true,
	"peerDependencies": {
		"typescript": "^5"
	}
}
